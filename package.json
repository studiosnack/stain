{
  "name": "insta",
  "version": "1.0.0",
  "license": "MIT",
  "volta": {
    "node": "22.5.1",
    "yarn": "1.22.22"
  },
  "scripts": {
    "dev": "nodemon src/server.js",
    "build:dev": "swc src --copy-files --out-dir dist --strip-leading-paths --only **/*.ts",
    "build:prod": "swc --cli-config-file prod.config.json src",
    "build:watch": "swc --only \"**/*.ts\" --copy-files --strip-leading-paths --out-dir dist --watch src"
  },
  "targets": {
    "frontend": {
      "source": "src/views/app.pug",
      "context": "browser",
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    },
    "server": {
      "source": "src/server.js",
      "context": "node",
      "engines": {
        "node": ">= 16"
      }
    }
  },
  "dependencies": {
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.4",
    "@swc/helpers": "^0.3.8",
    "@types/better-sqlite3": "^7.6.5",
    "better-sqlite3": "^11.1.2",
    "dotenv": "^16.3.1",
    "exif-reader": "2.0.1",
    "express": "^4.17.2",
    "express-session": "^1.17.3",
    "multer": "1.4.5-lts.1",
    "nanoid": "^3.1.30",
    "nodemon": "^2.0.21",
    "pug": "^3.0.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sharp": "0.32.1",
    "sqlite": "^4.0.25",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/multer": "^1.4.7",
    "prettier": "^2.8.8",
    "typescript": "^4.6.2"
  }
}
